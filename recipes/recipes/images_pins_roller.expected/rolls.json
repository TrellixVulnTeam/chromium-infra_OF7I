    "cwd": "[CACHE]/builder/checkout",
    "infra_step": true,
    "name": "read revision",
    "~followup_annotations": [
      "@@@STEP_TEXT@<br/>checked out 'deadbeef'<br/>@@@"
    ]
  },
  {
    "cmd": [
      "git",
      "clean",
      "-f",
      "-d",
      "-x"
    ],
    "cwd": "[CACHE]/builder/checkout",
    "infra_step": true,
    "name": "git clean"
  },
  {
    "cmd": [
      "vpython",
      "-u",
      "[CACHE]/infra_gclient_with_go/infra/go/bootstrap.py",
      "/path/to/tmp/json"
    ],
    "cwd": "[CACHE]/infra_gclient_with_go",
    "infra_step": true,
    "name": "init infra go env",
    "~followup_annotations": [
      "@@@STEP_TEXT@Using go 1.66.6@@@",
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"env\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"GOPATH\": \"[CACHE]/infra_gclient_with_go/infra/go\", @@@",
      "@@@STEP_LOG_LINE@json.output@    \"GOROOT\": \"[CACHE]/infra_gclient_with_go/golang/go\"@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"env_prefixes\": {@@@",
      "@@@STEP_LOG_LINE@json.output@    \"PATH\": [@@@",
      "@@@STEP_LOG_LINE@json.output@      \"[CACHE]/infra_gclient_with_go/golang/go\", @@@",
      "@@@STEP_LOG_LINE@json.output@      \"[CACHE]/infra_gclient_with_go/infra/go/bin\"@@@",
      "@@@STEP_LOG_LINE@json.output@    ]@@@",
      "@@@STEP_LOG_LINE@json.output@  }, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"go_version\": \"1.66.6\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "cloudbuildhelper",
      "version"
    ],
    "cwd": "[CACHE]/infra_gclient_with_go",
    "env": {
      "GOPATH": "[CACHE]/infra_gclient_with_go/infra/go",
      "GOROOT": "[CACHE]/infra_gclient_with_go/golang/go"
    },
    "env_prefixes": {
      "PATH": [
        "[CACHE]/infra_gclient_with_go/golang/go",
        "[CACHE]/infra_gclient_with_go/infra/go/bin"
      ]
    },
    "name": "cloudbuildhelper version",
    "~followup_annotations": [
      "@@@STEP_TEXT@<br/>cloudbuildhelper v6.6.6<br/><br/>CIPD package name: infra/tools/cloudbuildhelper/...<br/>CIPD instance ID:  lTJD7x...@@@"
    ]
  },
  {
    "cmd": [
      "cloudbuildhelper",
      "pins-update",
      "[CACHE]/builder/checkout/build/images/pins.yaml",
      "-json-output",
      "/path/to/tmp/json"
    ],
    "cwd": "[CACHE]/infra_gclient_with_go",
    "env": {
      "GOPATH": "[CACHE]/infra_gclient_with_go/infra/go",
      "GOROOT": "[CACHE]/infra_gclient_with_go/golang/go"
    },
    "env_prefixes": {
      "PATH": [
        "[CACHE]/infra_gclient_with_go/golang/go",
        "[CACHE]/infra_gclient_with_go/infra/go/bin"
      ]
    },
    "name": "cloudbuildhelper pins-update",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"updated\": [@@@",
      "@@@STEP_LOG_LINE@json.output@    \"some_image:tag\"@@@",
      "@@@STEP_LOG_LINE@json.output@  ]@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@"
    ]
  },
  {
    "cmd": [
      "git",
      "branch",
      "-D",
      "roll-pins"
    ],
    "cwd": "[CACHE]/builder/checkout",
    "infra_step": true,
    "name": "git branch"
  },
  {
    "cmd": [
      "git",
      "checkout",
      "-t",
      "origin/master",
      "-b",
      "roll-pins"
    ],
    "cwd": "[CACHE]/builder/checkout",
    "infra_step": true,
    "name": "git checkout (2)"
  },
  {
    "cmd": [
      "git",
      "commit",
      "-a",
      "-m",
      "Roll pinned docker image tags.\n\nUpdated pins:\n  * some_image:tag"
    ],
    "cwd": "[CACHE]/builder/checkout",
    "infra_step": true,
    "name": "git commit"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_REPO[depot_tools]/git_cl.py",
      "upload",
      "--bypass-hooks",
      "--force",
      "--tbrs=a@example.com",
      "--tbrs=b@example.com",
      "--use-commit-queue",
      "--message-file",
      "Roll pinned docker image tags.\n\nUpdated pins:\n  * some_image:tag"
    ],
    "cwd": "[CACHE]/builder/checkout",
    "name": "git cl upload"
  },
  {
    "cmd": [
      "python",
      "-u",
      "RECIPE_REPO[depot_tools]/git_cl.py",
      "issue",
      "--json",
      "/path/to/tmp/json"
    ],
    "cwd": "[CACHE]/builder/checkout",
    "name": "git cl issue",
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@json.output@{@@@",
      "@@@STEP_LOG_LINE@json.output@  \"issue\": 123456789, @@@",
      "@@@STEP_LOG_LINE@json.output@  \"issue_url\": \"https://chromium-review.googlesource.com/c/123456789\"@@@",
      "@@@STEP_LOG_LINE@json.output@}@@@",
      "@@@STEP_LOG_END@json.output@@@",
      "@@@STEP_LINK@Issue 123456789@https://chromium-review.googlesource.com/c/123456789@@@"
    ]
  },
  {
    "name": "$result"
  }
